  "description": "\u0623\u062c\u064a\u0628\u064a \u0639\u0646 \u0623\u0633\u0626\u0644\u062a\u0646\u0627 \u0644\u0645\u0639\u0631\u0641\u0629 \u0645\u062f\u0649 \u0630\u0643\u0627\u0626\u0643 \u0627\u0644\u0645\u0627\u0644\u064a!", 
  "language": "ara_AE", 
  "title": "\u0647\u0644 \u0623\u0646\u062a\u0650 \u0645\u062f\u062e\u0631\u0629 \u0645\u0645\u062a\u0627\u0632\u0629\u061f", 
  "primary_category": "38decbb6ab52480aa02218bca0ed92f5", 
  "created_at": "2016-04-26T10:39:08.454303+00:00", 
  "featured_in_category": false, 
  "modified_at": "2016-04-26T10:39:08.454546+00:00", 
  "_version": {
    "package_version": "1.4.4", 
    "language_version": "2.7.6", 
    "language_version_string": "2.7.6 (default, Jun 22 2015, 18:01:27) \n[GCC 4.8.2]", 
    "language": "python", 
